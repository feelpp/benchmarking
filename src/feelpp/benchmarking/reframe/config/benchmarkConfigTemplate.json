            {
                "name": "ElectricPostProcessing",
                "file": "electric.scalibility.ElectricPostProcessing.data",
                "format": "tsv"
            },
            {
                "name": "ElectricSolve",
                "file": "electric.scalibility.ElectricSolve.data",
                "format": "tsv"
            }
        ]
    },
    "sanity": {
        "success": [
        ],
        "error": [
        ]
    },
    "upload":{
        "active":true,
        "platform":"girder",
        "folder_id":"66ffd846b0e95728eb010353"
    },
    "parameters": {
        "nb_tasks": {
            "active": true,
            "type": "continuous",
            "topology": {
                "min_cores_per_node": 4,
                "max_cores_per_node": 16,
