{
    "leaves":{
        "default|gaya":{
            "contents":[{
                "type":"plot",
                "ref":"aggregated_data",
                "plot":{
                    "title":"Performance over time (Max over parameters)",
                    "plot_types":["scatter"],
                    "xaxis":{ "parameter":"date", "label":"Date" },
                    "yaxis": {"label":"Execution time (s)", "parameter":"value"},
                    "aggregations":[{"column":"elements|n|mesh|discretization|resources.tasks|tasks","agg":"max"}],
                    "color_axis":{"parameter":"perfvalue","label":"Performance Variable"}
                }
            }]
        },

